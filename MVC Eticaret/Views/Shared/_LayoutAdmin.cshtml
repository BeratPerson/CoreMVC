    <script>
        $(document).on("click", "[type='checkbox']", function (e) {
            if (this.checked) {
                $(this).attr("value", "true");
            } else {
                $(this).attr("value", "false");
            }
        });
    </script>

    <script>

        $('.mobile-close').click(function (e) {
            e.preventDefault();
            if ($('#sidebar-left').hasClass("active-mobile")) {
                $('#sidebar-left').removeClass("active-mobile");
            }
            else {
                $('#sidebar-left').addClass("active-mobile");
            }

        });
    </script>

